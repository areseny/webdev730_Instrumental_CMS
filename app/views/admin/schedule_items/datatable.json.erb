<% cache "schedule-items-datatable", skip_digest: true do %>
{
  "aaData": [
    <%= raw(@schedule_items.map do |item|
          date = link_to l(item.date, format: :brief), edit_admin_schedule_item_path(item)
          description = admin_json_description(item)
          [date, item.artist.try(:name), description, item.date.to_time.to_i].to_json
        end.join(',')) %>
  ]
}
<% end -%>
