<% cache "legacy-tv-shows-datatable", skip_digest: true do %>
{
  "aaData": [
    <%= raw(@legacy_tv_shows.map do |legacy_tv_show|
      date = link_to l(legacy_tv_show.date, format: :brief), edit_admin_legacy_tv_show_path(legacy_tv_show)
      description = admin_json_description(legacy_tv_show)
      [date, legacy_tv_show.artist.name, description, legacy_tv_show.date.to_time.to_i].to_json
    end.join(',')) %>
  ]
}
<% end -%>
